{
  "expo": {
    "name": "wrapper",
    "slug": "wrapper",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "wrapper",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logoBlack.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "ios": {
  "icon": "./assets/images/logoWrapper.png",
  "supportsTablet": true,
  "bundleIdentifier": "com.Kharshit801.wrapper",
  "buildNumber": "1.0.0",
  "infoPlist": {
    "LSApplicationQueriesSchemes": [
      "gpay",    
      "phonepe",
      "paytm",
      "bhim",
      "upi"
    ],
    "NSCameraUsageDescription": "This app needs camera access to scan QR codes for payments",
    "CFBundleURLTypes": [
      {
        "CFBundleURLSchemes": [
          "wrapper"
        ]
      }
    ]
  }
},
  "android": {
  "package": "com.Kharshit801.wrapper",
  "versionCode": 1,
  "adaptiveIcon": {
    "foregroundImage": "./assets/images/logo.png",
    "backgroundColor": "#1f1f1f"
  },
  "permissions": [
    "QUERY_ALL_PACKAGES"
  ],
  "queries": {
    "packages": [
      "com.google.android.apps.nbu.paisa.user",
      "net.one97.paytm",
      "com.phonepe.app",
      "in.org.npci.upiapp"
    ]
  },
  "intentFilters": [
    {
      "action": "VIEW",
      "data": [
        {
          "scheme": "upi"
        }
      ],
      "category": [
        "BROWSABLE",
        "DEFAULT"
      ]
    }
  ],
  "notifications": {
    "icon": "./assets/images/logo.png"
  }
},
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "1ec8fb3d-6d5b-4467-8226-e2dc033ef6c1"
      }
    }
  }
}
